<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>


<StackPane fx:id="rootPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="copaff.ui.link.LinkController">
   <children>
      <VBox fx:id="mainContainer" alignment="CENTER" prefHeight="388.0" prefWidth="588.0" spacing="10.0">
         <children>
            <JFXComboBox fx:id="squadName" maxWidth="1.7976931348623157E308" />
            <HBox spacing="10.0">
               <children>
                  <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                     <children>
                        <JFXComboBox fx:id="selectorPlayer1" maxWidth="1.7976931348623157E308" />
                        <JFXComboBox fx:id="selectorPlayer2" maxWidth="1.7976931348623157E308" />
                     </children>
                  </VBox>
                  <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                     <children>
                        <JFXComboBox fx:id="selectorPlayer3" maxWidth="1.7976931348623157E308" />
                        <JFXComboBox fx:id="selectorPlayer4" maxWidth="1.7976931348623157E308" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT">
               <children>
                  <JFXButton buttonType="RAISED" onAction="#assign" text="Assign" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
